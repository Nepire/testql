config samba
	option 'name'			'XIAO.MI'
	option 'workgroup'		'WORKGROUP'
	option 'description'		'Share from router'
	option 'homes'			'1'

config 'sambashare'
        option 'read_only' 'no'
        option 'create_mask' '0777'
        option 'dir_mask' '0777'
        option 'name' 'data'
        option 'path' '/tmp'
        option 'guest_ok' 'yes'
